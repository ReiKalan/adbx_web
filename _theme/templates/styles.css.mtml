body {
  background-image: url('<mt:SetVarBlock name="main_images"><$mt:CustomFieldValue identifier="main_images"$></mt:SetVarBlock><mt:If name="main_images"><mt:CustomFieldAsset identifier="main_images"><$mt:AssetURL$></mt:CustomFieldAsset><mt:Else><$mt:BlogURL encode_html="1"$>images/bg.jpg</mt:If>');
  background-size: cover;
  background-attachment: fixed;
  /* FIXME: Temp. workaround for vertical scrollbars */
  margin: 0;
}

body::before {
  background-size: cover;
  background-attachment: fixed;
  content: '';
  will-change: transform;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  position: fixed;
}

