/*-------------
  webpush
-------------*/
.webpush {
display: none;
} 

.mtnet-push-notification-service-enabled .webpush {
  display: block;
}

.side-webpush {
  background-color: #e0e0e0;
  text-decoration: none;
  font-weight: 500;
  font-size: 13px;
  display: block;
  -webkit-flex-shrink: 0;
  -ms-flex-negative: 0;
  flex-shrink: 0;
  padding: 16px 40px;
  margin: 0;
}
@media screen and (max-width: 1024px){
  .side-webpush {
    padding: 16px;
  }
}

.side-webpush a {
  color: #757575;
  text-decoration: none;
}

.side-webpush span::before {
  content: "\f0f3";
  font-size: 1.25em;
  font-family: FontAwesome;
  padding-right: 0.25em;
}
